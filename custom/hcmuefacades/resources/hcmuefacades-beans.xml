<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;hcmue.edu.vn.facades.product.data.GenderData>"/>
	</bean>

	<bean class="hcmue.edu.vn.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>

	<bean class="de.hybris.platform.hcmue.edu.vn.data.StadiumData">
		<description>Data object representing a Stadium</description>
		<property name="code" type="String"/>
		<property name="name" type="String"/>
		<property name="capacity" type="String"/>
		<property name="owner" type="String"/>
	</bean>

	<bean class="de.hybris.platform.hcmue.edu.vn.data.ClubData">
		<description>Data object representing a Club</description>
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>

</beans>
